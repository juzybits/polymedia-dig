{
	"name": "@polymedia/dig-web",
	"private": true,
	"scripts": {
		"clean": "rm -rf dist/ node_modules/ *.tsbuildinfo .turbo/ .wrangler/",
		"codegen": "sui-ts-codegen generate && ${npm_execpath} lint:fix",
		"deploy-dev": "$npm_execpath make && wrangler pages deploy dist/ --project-name polymedia-dig --branch dev",
		"deploy-prod": "$npm_execpath make && wrangler pages deploy dist/ --project-name polymedia-dig --branch main",
		"dev": "vite",
		"host": "vite --host",
		"lint:fix": "cp ../.gitignore . && biome check --write *.* src/ ; rm .gitignore",
		"lint:unsafe": "cp ../.gitignore . && biome check --write --unsafe *.* src/ ; rm .gitignore",
		"lint": "cp ../.gitignore . && biome check *.* src/ ; rm .gitignore",
		"make": "$npm_execpath typecheck && vite build",
		"preview": "$npm_execpath make && vite preview",
		"typecheck": "tsc -b --noEmit"
	},
	"dependencies": {
		"@mysten/dapp-kit": "^0.17.5",
		"@mysten/sui": "^1.37.4",
		"@polymedia/suitcase-core": "^0.0.66",
		"@polymedia/suitcase-react": "^0.0.65",
		"@tanstack/react-form": "^1.19.2",
		"@tanstack/react-query": "^5.85.5",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hot-toast": "^2.6.0",
		"react-router-dom": "^7.8.2",
		"three": "^0.179.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@mysten/codegen": "^0.4.3",
		"@types/react": "^19.1.11",
		"@types/react-dom": "^19.1.8",
		"@types/three": "^0.179.0",
		"@vitejs/plugin-react-swc": "^4.0.1",
		"typescript": "^5.9.2",
		"vite": "^7.1.3",
		"wrangler": "^4.32.0"
	},
	"type": "module",
	"sideEffects": false
}
